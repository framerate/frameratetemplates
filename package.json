{
  "name": "scl",
  "version": "2.0.0",
  "description": "Template for apps",
  "main": "build/app.js",
  "scripts": {
    "start": "node server.js",
    "build": "webpack -p --progress",
    "dev": "webpack --progress --color --watch & nodemon server/app.js"
  },
  "scriptsold": {
    "start": "NODE_ENV=development node server/app.js",
    "server": "NODE_ENV=production nodemon server/app.js -e js,handlebars",
    "build": "NODE_ENV=production webpack -p --config webpack.production.config.js --progress --colors",
    "clean": "rm -rf node_modules/ && npm i && npm start"
  },
  "author": "justin@secretcowlevel.com",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.3.16",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "dateformat": "^1.0.12",
    "flux": "^2.1.1",
    "hapi": "^11.1.4",
    "hapi-react-views": "^5.0.1",
    "inert": "^3.2.0",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "superagent": "^1.6.1",
    "vision": "^4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "nodemon": "^1.8.1",
    "webpack": "^1.12.9"
  }
}
